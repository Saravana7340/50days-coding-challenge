#User function Template for python3

class Solution:
	def is_palindrome(self, n):
	   x=n
	   sum=0
	   while n>0:
	       rem=n%10
	       sum=sum*10+rem
	       n=n//10
	   if x==sum:
	       return "Yes"
	   else:
	       return "No"
	       
	  
#{ 
 # Driver Code Starts
#Initial Template for Python 3

if __name__ == '__main__':
	T=int(input())
	for i in range(T):
		n = int(input())
		ob = Solution();
		ans = ob.is_palindrome(n)
		print(ans)
# } Driver Code Ends
